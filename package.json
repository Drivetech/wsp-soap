{
  "name": "wsp-soap",
  "version": "1.0.0",
  "description": "Dummy SOAP server",
  "main": "src",
  "scripts": {
    "start": "node src",
    "docker-start": "node src",
    "lint": "eslint src",
    "pretest": "npm run lint -s",
    "test": "mocha",
    "docker-test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lgaticaq/wsp-soap.git"
  },
  "engines": {
    "node": ">=5"
  },
  "author": "Leonardo Gatica <lgatica@protonmail.com> (https://about.me/lgatica)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lgaticaq/wsp-soap/issues"
  },
  "homepage": "https://github.com/lgaticaq/wsp-soap#readme",
  "dependencies": {
    "strong-soap": "^1.1.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "eslint": "7.28.0",
    "mocha": "5.1.1"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "indent": [
        2,
        2
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "quotes": [
        2,
        "single"
      ],
      "semi": [
        2,
        "always"
      ]
    }
  }
}
